{
  "compilerOptions": {
    "target": "ESNext", 
    "module": "ESNext", 
    "moduleResolution": "Node", 
    "lib": [ "ESNext", "DOM" ],
    "rootDir": "./", 
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "outDir": "../dist/dev",
    "strict": true, 
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
  },
}
