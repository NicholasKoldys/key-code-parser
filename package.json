{
  "name": "key-code-parser",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "private": "true",
  "scripts": {
    "watch:ts": "pnpm exec tsc --watch",
    "watch:node": "pnpm exec node --test --watch",
    "build:js": "pnpm exec tsc -p ./tsconfig.json",
    "build:pack": "pnpm exec webpack --config webpack.config.js",
    "pretest": "pnpm run build:dist",
    "test": "pnpm exec node --test \"./dist/_test/**/*.test.js\"",
    "prestart": "pnpm run test",
    "start": "pnpm run watch:ts",
    "serve": "pnpm exec node ./server.js",
    "build": "pnpm run build:js && pnpm run build:pack",
    "active-test": "pnpm run \"/^watch:.*/\""
  },
